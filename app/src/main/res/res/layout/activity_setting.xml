<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:orientation="vertical"
    tools:context="com.admin.gitframeapacas.Views.SettingActivity">


    <android.support.v7.widget.Toolbar android:id="@+id/toolbar" android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize" android:background="#90A4AE"
        app:layout_scrollFlags="scroll|enterAlways" app:popupTheme="@style/AppTheme.PopupOverlay"
        app:subtitleTextColor="#FFFFFF"
        app:titleTextColor="#FFFFFF"></android.support.v7.widget.Toolbar>

    <ListView android:id="@+id/setting_list" android:layout_width="match_parent"
        android:layout_height="match_parent"></ListView>
</LinearLayout>