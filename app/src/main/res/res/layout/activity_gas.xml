<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.ConstraintLayout android:layout_width="match_parent"
        android:layout_height="match_parent" android:orientation="vertical"
        tools:context=".Views.GraphGasActivity">

        <android.support.v7.widget.Toolbar android:id="@+id/toolbar" android:layout_width="0dp"
            android:layout_height="56dp" android:background="@color/colorPrimary"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_scrollFlags="scroll|enterAlways"
            app:popupTheme="@style/AppTheme.PopupOverlay" app:subtitleTextColor="#FFFFFF"
            app:titleTextColor="#FFFFFF" />

        <TextView android:id="@+id/txtCO" android:layout_width="344dp" android:layout_height="0dp"
            android:layout_marginTop="8dp" android:gravity="center" android:text="CO"
            android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/toolbar"
            tools:layout_editor_absoluteX="8dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartCO" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="8dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtCO" />


        <TextView android:id="@+id/txtNO2" android:layout_width="344dp" android:layout_height="0dp"
            android:layout_marginTop="16dp" android:gravity="center" android:text="NO2"
            android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/chartCO"
            tools:layout_editor_absoluteX="0dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartNO2" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="7dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtNO2" />


        <TextView android:id="@+id/txtO3" android:layout_width="344dp" android:layout_height="0dp"
            android:layout_marginTop="16dp" android:gravity="center" android:text="O3"
            android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/chartNO2"
            tools:layout_editor_absoluteX="0dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartO3" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="7dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtO3" />


        <TextView android:id="@+id/txtSO2" android:layout_width="344dp" android:layout_height="0dp"
            android:layout_marginTop="16dp" android:gravity="center" android:text="SO2"
            android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/chartO3"
            tools:layout_editor_absoluteX="0dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartSO2" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="7dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtSO2" />


        <TextView android:id="@+id/txtPM25" android:layout_width="344dp" android:layout_height="0dp"
            android:layout_marginTop="16dp" android:gravity="center" android:text="PM2.5"
            android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/chartSO2"
            tools:layout_editor_absoluteX="0dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartPM25" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="7dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtPM25" />


        <TextView android:id="@+id/txtRadio" android:layout_width="344dp"
            android:layout_height="0dp" android:layout_marginTop="16dp" android:gravity="center"
            android:text="RadioActive" android:textSize="20sp" android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/chartPM25"
            tools:layout_editor_absoluteX="0dp" />

        <org.eazegraph.lib.charts.ValueLineChart xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/chartRadio" android:layout_width="0dp" android:layout_height="200dp"
            android:layout_marginTop="7dp" app:egCurveSmoothness="0.4"
            app:egIndicatorLineColor="#FE6DA8" app:egLegendHeight="40dp"
            app:egShowStandardValue="true" app:egUseCubic="true" app:egUseOverlapFill="true"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtRadio" />

    </android.support.constraint.ConstraintLayout>
</android.support.v4.widget.NestedScrollView>